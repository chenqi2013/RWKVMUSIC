    "version": "0.2.0",
    "configurations": [
        {
            "name": "rwkvmusic",
            "cwd": "rwkvmusic",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "rwkvmusic (profile mode)",
            "cwd": "rwkvmusic",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "rwkvmusic (release mode)",
            "cwd": "rwkvmusic",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "C++ Debugger (Windows)",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}\\rwkvmusic\\build\\windows\\x64\\runner\\Debug\\rwkvmusic.exe",
            "cwd": "${workspaceFolder}\\rwkvmusic",
            "externalConsole": false,
            "logging": {
                "moduleLoad": false,
                "trace": true
            },
        },
    ],
}
